{
  "compilerOptions": {
    "module": "commonjs",
    "typeRoots": [
      "@types",
      "node_modules/@types"
    ],
    "target": "es2016",
    "lib": [
      "dom",
      "esnext",
      "es6"
    ],
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "strict": false,
    "suppressImplicitAnyIndexErrors": true
  }
}